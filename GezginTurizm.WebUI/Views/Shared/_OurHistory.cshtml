@model SliderPhotoGalleryModel

    <section id="about" class="about">
        <div class="container">
            <div class="section-title">
                <h2>Gezgin Turizm</h2>
                <b>Tarihçemiz</b>
            </div>
            <div class="row content">
                <div class="col-lg-6">
                    <img src="~/img/about.jpg" class="img-fluid" alt="">
                </div>
                <div class="col-lg-6 pt-4 pt-lg-0">
                    <ul>
                        @foreach (var item in Model.OurHistories)
                        {
                            <li><i class="ri-check-double-line"></i>@item.Text</li>
                        }
                    </ul>
                    <a href="/Home/Institutional" class="btn-learn-more">Devamını Gör</a>
                </div>
            </div>
        </div>
    </section>